{
  "name": "voi-nft-games-dashboard",
  "version": "1.0.0",
  "descrption": "Voi NFT Olympics Dashboard",
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "build": "yarn typecheck && vite build",
    "dev": "concurrently \"vite --host\" \"node ./server/api-server.mjs\"",
    "format": "prettier --check --ignore-path .gitignore --write .",
    "lint": "eslint --ignore-path .gitignore .",
    "preview": "vite preview --host",
    "typecheck": "tsc"
  },
  "engines": {
    "node": ">= 16",
    "npm": ">= 8",
    "yarn": ">= 1"
  },
  "dependencies": {
    "@agoralabs-sh/avm-web-provider": "^1.7.0",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@headlessui/react": "^1.7.18",
    "@headlessui/tailwindcss": "^0.2.0",
    "@heroicons/react": "^2.1.1",
    "@mui/material": "^5.15.7",
    "@nextui-org/react": "^2.2.10",
    "@remixicon/react": "^4.1.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tremor/react": "^3.13.4",
    "@txnlab/use-wallet": "^3.10.0",
    "@txnlab/use-wallet-react": "^3.10.0",
    "algosdk": "^2.7.0",
    "apexcharts": "^3.36.3",
    "axios": "^1.6.7",
    "better-sqlite3": "^9.4.5",
    "buffer": "^6.0.3",
    "canvas-confetti": "^1.9.2",
    "classnames": "^2.3.2",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.2",
    "express": "^4.19.2",
    "flowbite": "^1.5.5",
    "flowbite-react": "^0.7.2",
    "framer-motion": "^11.0.22",
    "install": "^0.13.0",
    "latest": "^0.2.0",
    "minimist": "^1.2.8",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.18.1",
    "mysql2": "^3.9.1",
    "node-schedule": "^2.1.1",
    "nodemailer": "^6.9.9",
    "nodemailer-smtp-transport": "^2.7.4",
    "npm": "^10.4.0",
    "papaparse": "^5.4.1",
    "react-apexcharts": "^1.4.0",
    "react-icons": "4.7.1",
    "react-joyride": "^2.7.2",
    "react-map-gl": "^7.1.7",
    "react-toastify": "^10.0.4",
    "react-transition-group": "^4.4.5",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "tailwind-merge": "^2.2.1",
    "ulujs": "^0.11.16"
  },
  "devDependencies": {
    "@types/canvas-confetti": "^1.6.4",
    "@types/papaparse": "^5.3.14",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/sortablejs": "^1.15.0",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "@vitejs/plugin-react": "^2.2.0",
    "autoprefixer": "^10.4.13",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.7.1",
    "postcss": "^8.4.19",
    "prettier": "^2.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.22.0",
    "react-router-dom": "^6.22.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4",
    "vite": "^3.2.5",
    "vitest": "^0.25.6"
  },
  "private": true,
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/strict",
      "plugin:jsx-a11y/recommended",
      "plugin:prettier/recommended",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended",
      "plugin:tailwindcss/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "project": "./tsconfig.json"
    },
    "root": true,
    "rules": {
      "@typescript-eslint/consistent-type-imports": "error",
      "react/react-in-jsx-scope": "off",
      "react/no-unescaped-entities": "off",
      "tailwindcss/classnames-order": [
        "error",
        {
          "callees": [
            "classnames"
          ],
          "config": "tailwind.config.cjs"
        }
      ],
      "tailwindcss/no-custom-classname": [
        "error",
        {
          "config": "tailwind.config.cjs"
        }
      ]
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  }
}
